{
    "agent": {
        "metrics_collection_interval": 10,
        "run_as_user": "cwagent"
    },
    "metrics": {
        "namespace": "Containers/Development/Process",
        "aggregation_dimensions": [["ContainerId"]],
        "metrics_collected": {
            "procstat": [
                {
                    "exe": "prefetch.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                },
                {
                    "exe": "fasterq-dump.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                },
                {
                    "exe": "salmon.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                },
                {
                    "exe": "R.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                }

            ]
        }
    }
}