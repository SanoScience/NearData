{
    "agent": {
        "metrics_collection_interval": 10,
        "run_as_user": "cwagent"
    },
    "metrics": {
        "namespace": "Containers/Development/Process",
        "aggregation_dimensions": [["ContainerId"]],
        "metrics_collected": {
            "procstat": [
                {
                    "exe": "prefetch.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                },
                {
                    "exe": "fasterq-dump.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                },
                {
                    "exe": "salmon.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                },
                {
                    "exe": "R.*",
                    "measurement": [
                        "cpu_time",
                        "cpu_time_idle",
                        "cpu_time_iowait",
                        "cpu_time_user",
                        "cpu_usage",
                        "memory_rss",
                        "memory_vms",
                        "memory_swap",
                        "num_threads",
                        "read_bytes",
                        "write_bytes",
                        "read_count",
                        "write_count"
                    ],
                    "append_dimensions": {
                        "ContainerId": "$HOSTNAME"
                    }
                }

            ],
            "cpu": {
                "measurement": [
                    "cpu_usage_idle",
                    "cpu_usage_iowait",
                    "cpu_usage_user"
                ],
                "metrics_collection_interval": 10,
                "resources": [
                    "*"
                ],
                "totalcpu": true,
                "append_dimensions": {
                    "ContainerId": "$HOSTNAME"
                }
            },
            "disk": {
                "measurement": [
                    "used_percent"
                ],
                "metrics_collection_interval": 10,
                "resources": [
                    "*"
                ],
                "drop_device": true,
                "append_dimensions": {
                    "ContainerId": "$HOSTNAME"
                }
            },
            "diskio": {
                "measurement": [
                    "io_time",
                    "write_bytes",
                    "read_bytes",
                    "writes",
                    "reads"
                ],
                "metrics_collection_interval": 10,
                "resources": [
                    "*"
                ],
                "append_dimensions": {
                    "ContainerId": "$HOSTNAME"
                }
            },
            "mem": {
                "measurement": [
                    "mem_used",
                    "mem_used_percent"
                ],
                "metrics_collection_interval": 10,
                "append_dimensions": {
                    "ContainerId": "$HOSTNAME"
                }
            },
            "net": {
                "measurement": [
                    "bytes_sent",
                    "bytes_recv",
                    "packets_sent",
                    "packets_recv"
                ],
                "metrics_collection_interval": 10,
                "resources": [
                    "*"
                ],
                "append_dimensions": {
                    "ContainerId": "$HOSTNAME"
                }
            },
            "statsd": {
                "metrics_aggregation_interval": 0,
                "metrics_collection_interval": 10,
                "service_address": ":8125"
            },
            "swap": {
                "measurement": [
                    "swap_used_percent"
                ],
                "metrics_collection_interval": 10,
                "append_dimensions": {
                    "ContainerId": "$HOSTNAME"
                }
            }
        }
    }
}